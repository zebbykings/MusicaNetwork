<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">



	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/public/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>consSection</from-outcome>
			<to-view-id>/pages/public/cuser_index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>musSection</from-outcome>
			<to-view-id>/pages/public/muser_index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/public/cuser_index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>CUserLoggedIn</from-outcome>
			<to-view-id>/pages/protected/cuser/cuser_home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>



</faces-config>